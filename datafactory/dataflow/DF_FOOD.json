{
	"name": "DF_FOOD",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_ADLS_FOOD_PARQUET",
						"type": "DatasetReference"
					},
					"name": "SourceFood",
					"description": "Import data from DS_ADLS_PARQUET"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_ADLS_FOOD_PARQUET",
						"type": "DatasetReference"
					},
					"name": "SinkHubFood"
				}
			],
			"transformations": [
				{
					"name": "DerivedColumn1"
				},
				{
					"name": "HubFood"
				}
			],
			"scriptLines": [
				"source(output(",
				"          N as string,",
				"          f_id as string,",
				"          item as string,",
				"          veg_or_non_veg as string,",
				"          f_actuliz as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> SourceFood",
				"SourceFood derive(Food_Id = f_id) ~> DerivedColumn1",
				"DerivedColumn1 select(mapColumn(",
				"          Food_Id,",
				"          item,",
				"          veg_or_non_veg,",
				"          f_actuliz",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> HubFood",
				"HubFood sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SinkHubFood"
			]
		}
	}
}